{
    "compilerOptions": {
        "sourceMap": true,
        "target": "ES2015",
        "strict": true,
        "module": "commonJs",
        "jsx": "react",
        "lib": [
            "esnext",
            "dom"
        ],
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
    }
}